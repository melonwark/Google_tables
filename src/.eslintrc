{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style":"off",
    "require-jsdoc":"off",
    "indent":"off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module"
}
}